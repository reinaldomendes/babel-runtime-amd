{
    "presets": [
        ["@babel/preset-env", {
            "exclude": [
                "transform-regenerator",
            ]
        }]
    ],
    "plugins": [
        ["@babel/plugin-transform-modules-amd", {
            "noInterop": true
        }],
        ["@babel/plugin-transform-runtime", {
            "absoluteRuntime": false,
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "version": "^7.14.5",
            "useESModules": false
        }],
        ["@babel/plugin-transform-classes"]
    ]
}
